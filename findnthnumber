# Function for nth Fibonacci number
def fbncii(n):

    # input is 0 then it will print incorrect input
    if n < 0:
        print("Incorrect input")

    # n is 0 then it will return 0
    elif n == 0:
        return 0

    # if n is 1,2 it will return 1
    elif n == 1 or n == 2:
        return 1

    else:
        return fbnciii(n-1) + fbncii(n-2)
print(Fibonacci(10))
